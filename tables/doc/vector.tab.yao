{
    "action": {
        "bind": {
            "model": "doc.vector",
            "option": {
                "option": {
                    "form": "doc.vector"
                },
                "withs": {
                    "file": {}
                }
            }
        }
    },
    "fields": {
        "filter": {
            "文件ID": {
                "bind": "where.file_id.in",
                "edit": {
                    "props": {
                        "itemProps": {
                            "rules": [
                                {
                                    "type": "integer"
                                }
                            ],
                            "tooltip": "文件ID"
                        },
                        "xProps": {
                            "$remote": {
                                "process": "yao.component.SelectOptions",
                                "query": {
                                    "label": "filename",
                                    "model": "doc.file",
                                    "value": "id"
                                }
                            }
                        }
                    },
                    "type": "Select"
                }
            }
        },
        "table": {
            "ID": {
                "bind": "id",
                "edit": {
                    "bind": "id",
                    "props": {
                        "itemProps": {
                            "tooltip": "ID"
                        }
                    },
                    "type": "Input"
                },
                "view": {
                    "props": {},
                    "type": "Text"
                }
            },
            "内容": {
                "bind": "content",
                "edit": {
                    "bind": "content",
                    "props": {
                        "itemProps": {
                            "tooltip": "内容"
                        }
                    },
                    "type": "TextArea"
                },
                "view": {
                    "props": {},
                    "type": "Text"
                }
            },
            "嵌入": {
                "bind": "embedding",
                "edit": {
                    "bind": "embedding",
                    "props": {
                        "itemProps": {
                            "tooltip": "嵌入"
                        }
                    },
                    "type": "TextArea"
                },
                "view": {
                    "props": {},
                    "type": "Text"
                }
            },
            "文件ID": {
                "bind": "file_id",
                "edit": {
                    "props": {
                        "itemProps": {
                            "rules": [
                                {
                                    "type": "integer"
                                }
                            ],
                            "tooltip": "文件ID"
                        },
                        "xProps": {
                            "$remote": {
                                "process": "yao.component.SelectOptions",
                                "query": {
                                    "label": "filename",
                                    "model": "doc.file",
                                    "value": "id"
                                }
                            }
                        }
                    },
                    "type": "Select"
                },
                "view": {
                    "props": {
                        "xProps": {
                            "$remote": {
                                "process": "yao.component.SelectOptions",
                                "query": {
                                    "label": "filename",
                                    "model": "doc.file",
                                    "value": "id"
                                }
                            }
                        }
                    },
                    "type": "Tag"
                }
            },
            "索引": {
                "bind": "index",
                "edit": {
                    "bind": "index",
                    "props": {
                        "itemProps": {
                            "rules": [
                                {
                                    "type": "integer"
                                }
                            ],
                            "tooltip": "索引"
                        }
                    },
                    "type": "InputNumber"
                },
                "view": {
                    "props": {},
                    "type": "Text"
                }
            }
        }
    },
    "layout": {
        "filter": {
            "actions": [
                {
                    "action": [
                        {
                            "name": "OpenModal",
                            "payload": {
                                "Form": {
                                    "model": "doc.vector",
                                    "type": "edit"
                                }
                            },
                            "type": "Common.openModal"
                        }
                    ],
                    "icon": "icon-plus",
                    "title": "添加",
                    "width": 3
                }
            ],
            "columns": [
                {
                    "name": "文件ID",
                    "width": 4
                }
            ]
        },
        "header": {
            "actions": [],
            "preset": {}
        },
        "primary": "id",
        "table": {
            "columns": [
                {
                    "name": "ID",
                    "width": 160
                },
                {
                    "name": "索引",
                    "width": 160
                },
                {
                    "name": "文件ID",
                    "width": 160
                },
                {
                    "name": "内容",
                    "width": 160
                },
                {
                    "name": "嵌入",
                    "width": 160
                }
            ],
            "operation": {
                "actions": [
                    {
                        "action": [
                            {
                                "name": "OpenModal",
                                "payload": {
                                    "Form": {
                                        "model": "doc.vector_view",
                                        "type": "view"
                                    }
                                },
                                "type": "Common.openModal"
                            }
                        ],
                        "icon": "icon-eye",
                        "title": "查看"
                    },
                    {
                        "action": [
                            {
                                "name": "OpenModal",
                                "payload": {
                                    "Form": {
                                        "model": "doc.vector",
                                        "type": "edit"
                                    }
                                },
                                "type": "Common.openModal"
                            }
                        ],
                        "icon": "icon-edit-2",
                        "title": "编辑"
                    },
                    {
                        "action": [
                            {
                                "name": "Confirm",
                                "payload": {
                                    "content": "删除后不可撤销！",
                                    "title": "确认删除"
                                },
                                "type": "Common.confirm"
                            },
                            {
                                "name": "Delete",
                                "payload": {
                                    "model": "doc.vector"
                                },
                                "type": "Table.delete"
                            }
                        ],
                        "icon": "icon-trash-2",
                        "title": "删除"
                    }
                ],
                "fold": false
            }
        }
    },
    "name": "doc_vector"
}