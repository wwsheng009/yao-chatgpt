{
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "yao",
      "database": "./db/yao.db"
    }
  ],
  "json.schemas": [
    {
      "fileMatch": ["/**/*.mod.json", "/**/*.mod.yao"],
      "url": "/json-schemas/model.json"
    },
    {
      "fileMatch": ["/**/*app.json", "/**/*app.yao"],
      "url": "/json-schemas/app.json"
    },
    {
      "fileMatch": ["/**/*.login.json", "/**/*.login.yao"],
      "url": "/json-schemas/login.json"
    },
    {
      "fileMatch": ["/**/*.schedule.json", "/**/*.schedule.yao"],
      "url": "/json-schemas/schedule.json"
    },
    {
      "fileMatch": ["/**/*.service.json", "/**/*.service.yao"],
      "url": "/json-schemas/service.json"
    },
    {
      "fileMatch": ["/**/*.socket.json", "/**/*.socket.yao"],
      "url": "/json-schemas/socket.json"
    },
    {
      "fileMatch": ["/**/*.stor.json", "/**/*.stor.yao"],
      "url": "/json-schemas/store.json"
    },
    {
      "fileMatch": ["/**/*.task.json", "/**/*.task.yao"],
      "url": "/json-schemas/task.json"
    },
    {
      "fileMatch": ["/**/*.widget.json", "/**/*.widget.yao"],
      "url": "/json-schemas/widget.json"
    },
    {
      "fileMatch": ["/**/apis/**/*.ws.json", "/**/apis/**/*.ws.yao"],
      "url": "/json-schemas/ws_server.json"
    },
    {
      "fileMatch": [
        "/**/websockets/**/*.ws.json",
        "/**/websockets/**/*.ws.yao"
      ],
      "url": "/json-schemas/ws_client.json"
    },
    {
      "fileMatch": ["/**/*.imp.json", "/**/*.imp.yao"],
      "url": "/json-schemas/importer.json"
    },
    {
      "fileMatch": ["/**/*.http.json", "/**/*.http.yao"],
      "url": "/json-schemas/api_http.json"
    },
    {
      "fileMatch": ["/**/*.rest.json", "/**/*.rest.yao"],
      "url": "/json-schemas/api_rest.json"
    },
    {
      "fileMatch": ["/**/*.chart.json", "/**/*.chart.yao"],
      "url": "/json-schemas/chart.json"
    },
    {
      "fileMatch": ["/**/*.tab.json", "/**/*.tab.yao"],
      "url": "/json-schemas/table.json"
    },
    {
      "fileMatch": ["/**/*.conn.json", "/**/*.conn.yao"],
      "url": "/json-schemas/connector.json"
    },
    {
      "fileMatch": ["/**/*.model.json", "/**/*.model.yao"],
      "url": "/json-schemas/model.json"
    },
    {
      "fileMatch": [
        "/**/*.form.json",
        "/**/*.form.yao",
        "!/**/dyforms/**/*.form.json",
        "!/**/dyforms/**/*.form.yao"
      ],
      "url": "/json-schemas/form.json"
    },
    {
      "fileMatch": ["/**/*.flow.json", "/**/*.flow.yao"],
      "url": "/json-schemas/flow.json"
    },
    {
      "fileMatch": ["/**/*.dash.json", "/**/*.dash.yao"],
      "url": "/json-schemas/dashboard.json"
    }
  ]
}
