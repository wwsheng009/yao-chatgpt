{
  "name": "::AI Model",
  "action": {
    "bind": {
      "model": "ai.model",
      "option": {
        "withs": {
          "permissions": {}
        }
      }
    }
  },
  "layout": {
    "actions": [
      {
        "title": "返回",
        "icon": "icon-arrow-left",
        "showWhenAdd": true,
        "showWhenView": true,
        "action": [
          {
            "name": "CloseModal",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "title": "保存",
        "icon": "icon-check",
        "style": "primary",
        "showWhenAdd": true,
        "action": [
          {
            "name": "Submit",
            "type": "Form.submit",
            "payload": {}
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "action": [
          {
            "name": "Delete",
            "payload": {
              "model": "ai.setting"
            },
            "type": "Form.delete"
          }
        ],
        "confirm": {
          "desc": "请确认删除，删除后数据无法恢复",
          "title": "确认"
        },
        "icon": "icon-trash-2",
        "style": "danger",
        "title": "Delete"
      }
    ],
    "form": {
      "sections": [
        {
          "columns": [
            {
              "name": "IDX",
              "width": 12
            },
            {
              "name": "回答",
              "width": 12
            },
            {
              "name": "拥有者",
              "width": 12
            },
            {
              "name": "创建时间",
              "width": 12
            },
            {
              "name": "上级",
              "width": 12
            },
            {
              "name": "根节点",
              "width": 12
            },
            {
              "name": "权限",
              "width": 24
            }
          ]
        }
      ]
    }
  },
  "fields": {
    "form": {
      "权限": {
        "bind": "permissions",
        "edit": {
          "type": "List",
          "props": {
            "name": "permission",
            "showLabel": true
          }
        }
      }
    }
  }
}