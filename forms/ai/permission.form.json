{
  "name": "::AI API Setting",
  "action": {
    "bind": {
      "model": "ai.permission",
      "option": {
        "withs": {
          "model": {}
        }
      }
    }
  },
  "layout": {
    "actions": [
      {
        "title": "返回",
        "icon": "icon-arrow-left",
        "showWhenAdd": true,
        "showWhenView": true,
        "action": [
          {
            "name": "CloseModal",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "title": "保存",
        "icon": "icon-check",
        "style": "primary",
        "showWhenAdd": true,
        "action": [
          {
            "name": "Submit",
            "type": "Form.submit",
            "payload": {}
          },
          {
            "name": "Back",
            "type": "Common.closeModal",
            "payload": {}
          }
        ]
      },
      {
        "action": [
          {
            "name": "Delete",
            "payload": {
              "model": "ai.setting"
            },
            "type": "Form.delete"
          }
        ],
        "confirm": {
          "desc": "请确认删除，删除后数据无法恢复",
          "title": "确认"
        },
        "icon": "icon-trash-2",
        "style": "danger",
        "title": "Delete"
      }
    ]
  }
}