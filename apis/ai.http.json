{
  "name": "AI Chat API",
  "version": "1.0.0",
  "description": "User API",
  "group": "ai",
  "guard": "-",
  "paths": [
    {
      "path": "/ask",
      "method": "GET",
      "guard": "-",
      "process": "scripts.ai.chatgpt.Callq",
      "in": ["$query.q"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/ask",
      "method": "POST",
      "guard": "-",
      "process": "scripts.ai.chatgpt.Call",
      "in": [":payload"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/ask-stream",
      "method": "POST",
      "guard": "-",
      "process": "scripts.ai.stream.Call",
      "in": [":payload"],
      "out": {
        "stream": true,
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/ask2",
      "method": "POST",
      "guard": "-",
      "process": "scripts.ai.chatgpt2.Call",
      "in": [":payload"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/context",
      "method": "GET",
      "guard": "-",
      "process": "scripts.utils.test.test",
      "in": [":context"],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/templates",
      "method": "GET",
      "guard": "-",
      "process": "flows.chat.prompt_template",
      "in": [""],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    },
    {
      "path": "/template/:id",
      "method": "GET",
      "guard": "-",
      "process": "models.chat.prompttemplate.find",
      "in": ["$param.id", ""],
      "out": {
        "status": 200,
        "type": "application/json",
        "body": "{{content}}"
      }
    },
    {
      "path": "/setting",
      "method": "GET",
      "guard": "scripts.security.CheckAccessKey",
      "process": "scripts.ai.chatgpt.GetSetting",
      "in": [],
      "out": {
        "status": 200,
        "type": "application/json"
      }
    }
  ]
}
